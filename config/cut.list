# lines beginning with '# ' are comments
# C cut_number cut_label \n cut_value
C 0 no_cut

C 1 basic cut
lep_pt > 50 && MET > 65 && lv_J_delta_phi > 2 && MET_J_delta_phi > 2 && l_J_delta_R > 3.14/2 && lv_Mt > 30 
C 2 Wh_mass_window
CA8_jet_mass > 65 && CA8_jet_mass < 105 
C 3 Wl_mass_window
lv_mass > 65 && lv_mass < 105 
C 4 boosted Ws
CA8_jet_pt > 200 && lv_pt > 200
C 5 b_veto 
vbf_jet1_btag < 0.5 && vbf_jet2_btag < 0.5
C 6 top_mass_veto 
lv_closerjet_mass > 200 && CA8_closerjet_mass > 210
C 7 W_tagger
CA8_jet_t2t1 < 0.5
C 8 vbf_topology
TMath::Abs(vbf_jet1_eta - vbf_jet2_eta) > 2.5 && vbf_jj_mass > 250 
C 9 b_tag
(vbf_jet1_btag > 0.5 || vbf_jet2_btag > 0.5)